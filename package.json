{
  "name": "spring-framework",
  "version": "4.3.2",
  "description": "Spring.io core port for javascript",
  "author": "Arsenii_Boretskii <arseniiboretskii@gmail.com>",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "dist",
    "test": "src/__tests__"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ArseniyBorezkiy/js-beans"
  },
  "scripts": {
    "assemble": "rimraf dist && tsc && babel dist -d dist",
    "build": "rollup -c rollup.lib.config.js",
    "test": "jest --no-cache",
    "doc": "typedoc --out ./docs --exclude **/*tests*/* ./src",
    "test:clear": "jest --clearCache"
  },
  "peerDependencies": {
    "reflect-metadata": "0.1.12"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@types/fetch-mock": "^7.2.3",
    "@types/jest": "24.0.11",
    "babel-loader": "^8.0.6",
    "fetch-mock": "^7.3.1",
    "jest": "29.3.1",
    "jest-fetch-mock": "^2.1.2",
    "reflect-metadata": "0.1.12",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-copy": "^1.1.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-typescript": "^1.0.0",
    "rollup-plugin-typescript2": "^0.20.1",
    "ts-jest": "23.10.5",
    "tslib": "^1.9.3",
    "typedoc": "^0.14.2",
    "typescript": "3.2.2"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts)$": "ts-jest"
    },
    "testRegex": "/__tests__/.*\\.(ts)$",
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json",
        "diagnostics": false,
        "compilerOptions": {
          "importHelpers": true
        }
      }
    },
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ],
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ]
  },
  "keywords": [
    "spring",
    "framework",
    "IoC"
  ],
  "license": "ISC"
}
